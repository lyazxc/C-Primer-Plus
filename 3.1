 /*品脱 杯 盎司 大汤勺 茶勺*/
 #include<stdio.h>
int main(void)
{
	float a;  /*品脱*/
	int b;    /*杯*/ 
	int c;    /*盎司*/ 
	int d;    /*大汤勺*/ 
	int e;    /*茶勺*/ 
	printf("Enter your date: ");
	scanf("%d" ,&b);
	a = b * 0.5;  /*品脱*/ 
	c = b * 8;    /*盎司*/ 
	d = c * 2;    /*大汤勺*/ 
	e = d * 3;     /*茶勺*/ 
	printf("品脱=%f 杯=%d 盎司=%d 大汤勺=%d 茶勺=%d", a,b,c,d,e);
	return 0;
 } 
