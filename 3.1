 /*夸脱水分子*/
 #include<stdio.h>
int main(void)
{
	float water;
	float quart;
	float molecular;
	float in;
	water = 3.0e-23;
	quart = 950;
	printf("Enter your quart: ");
	scanf("%f",&in);
	molecular = in * quart / water;  
	printf("%f quart %e molecular",in ,molecular);
	return 0;
 } 
